{
  "name": "@grafana/openapi-to-k6",
  "version": "0.1.1",
  "description": "A CLI tool to generate helper modules for K6 from OpenAPI schema",
  "main": "dist/cli.js",
  "bin": {
    "openapi-to-k6": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/cli.ts",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "lint": "eslint --ext .ts,.tsx .",
    "lint:fix": "eslint --ext .ts,.tsx . --fix",
    "format": "prettier --ignore-unknown --write \"./**/*\"",
    "update-examples": "for dir in ./examples/*/; do (npm run dev -- \"$dir/schema.json\" \"$dir\"); done",
    "prepare": "husky"
  },
  "author": "",
  "license": "AGPL-3.0",
  "dependencies": {
    "@types/k6": "^0.54.0",
    "axios": "^1.7.7",
    "chalk": "^4.1.2",
    "commander": "^12.1.0",
    "handlebars": "^4.7.8",
    "openapi-typescript": "^7.4.1",
    "orval": "^7.1.1",
    "prettier": "^3.3.3",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.13",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --ignore-unknown --write",
      "bash -c 'tsc --noEmit'"
    ]
  }
}
